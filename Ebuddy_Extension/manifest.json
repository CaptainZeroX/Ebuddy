{
    "name": "Ebuddy",
    "description": "An Extension That Helps you with your everyday needs",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting", "webRequest"],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "/images/16.png",
        "32": "/images/32.png",
        "128": "/images/128.png"
      }
    },
    "icons": {
      "16": "/images/16.png",
      "32": "/images/32.png",
      "128": "/images/128.png"
    }
  }
  